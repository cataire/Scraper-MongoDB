

<div class = "headlines">
  {{#each headlines}}
  {{#unless saved}}
  <div class="card">
      <div class="card-body">
        <div class="row">
          <div class="col-md-9">
        <h5 class="card-title"><a href = "{{link}}" target="_blank">{{title}}</a></h5>

      </div>
      <div class="col-md-3">

    <button class = "save-headline" data-id = "{{id}}">Save Article</button>
      </div>
      </div>
      </div>
    </div>
    {{/unless}}
      {{/each}}
</div>
